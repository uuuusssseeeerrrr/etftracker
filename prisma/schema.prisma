generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model etf_list {
  market          String           @default("") @db.VarChar(10)
  stock_code      String           @default("") @db.VarChar(20)
  etf_name        String?          @db.VarChar(255)
  company_name    String?          @db.VarChar(50)
  benchmark_index String?          @db.VarChar(255)
  index_comment   String?          @db.Text
  trading_lot     String?          @db.VarChar(10)
  trust_fee_rate  String?          @db.VarChar(10)
  std_pdno        String?          @db.VarChar(12)
  reg_date        DateTime?        @db.DateTime(0)
  mod_date        DateTime?        @db.DateTime(0)
  etf_stock_list  etf_stock_list[]

  @@id([market, stock_code])
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model etf_price_history {
  price_idx      Int       @id @default(autoincrement())
  market         String    @db.VarChar(10)
  stock_code     String    @db.VarChar(20)
  open           String?   @db.VarChar(12)
  high           String?   @db.VarChar(12)
  low            String?   @db.VarChar(12)
  price          String?   @db.VarChar(12)
  last_day_price String?   @db.VarChar(12)
  h52p           String?   @db.VarChar(12)
  l52p           String?   @db.VarChar(12)
  t_xprc         String?   @db.VarChar(12)
  t_xdif         String?   @db.VarChar(12)
  t_xrat         String?   @db.VarChar(12)
  t_rate         String?   @db.VarChar(12)
  reg_date       DateTime? @db.DateTime(0)

  @@index([market, stock_code], map: "market_stock_code")
  @@index([reg_date], map: "reg_date")
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model etf_stock_list {
  market         String     @default("") @db.VarChar(10)
  etf_stock_code String     @default("") @db.VarChar(20)
  stock_code     String     @default("") @db.VarChar(20)
  reg_date       DateTime?  @db.DateTime(0)
  etf_percent    Float?     @db.Float
  etf_list       etf_list   @relation(fields: [market, etf_stock_code], references: [market, stock_code], onUpdate: Restrict, map: "FK_etf_stock_list_etf_list")
  stock_list     stock_list @relation(fields: [market, stock_code], references: [market, stock_code], onUpdate: Restrict, map: "etf_stock_list_stock_list_market_stock_code_fk")

  @@id([market, etf_stock_code, stock_code])
  @@index([market, stock_code], map: "etf_stock_list_stock_list_market_stock_code_fk")
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model stock_list {
  market         String           @default("") @db.VarChar(10)
