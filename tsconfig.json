{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "#models/*": ["./models/*"],
      "#server/*": ["./server/*"],
      "#types/*": ["./types/*"],
      "#lib/*": ["./lib/*"],
      "#util/*": ["./server/utils/*"]
    },
    "module": "ESNext",
    "moduleResolution": "node",
    "target": "ES2023",
    "strict": true,
    "esModuleInterop": true
  },
}
